% Example compile-time options: 
%  (HYDRO_MESHLESS_FINITE_MASS is optional, replace with your 
%   choice of hydro/mhd options)
%
%        HYDRO_MESHLESS_FINITE_MASS
%        BOX_PERIODIC
%        BOX_LONG_X=16
%        BOX_LONG_Y=1
%        BOX_LONG_Z=1
%        BOX_SPATIAL_DIMENSION=2
%        EOS_GAMMA=(2.0)
%        MAGNETIC
%        SELFGRAVITY_OFF
%
InitCondFile                       briowu_ics
ICFormat                           3
OutputDir                          output
TimeLimitCPU                       50000
MaxMemSize                         1600
PartAllocFactor                    5
BufferSize                         150
TimeBegin                          0
TimeMax                            0.2
ComovingIntegrationOn              0
BoxSize                            0.25
OutputListOn                       0
TimeOfFirstSnapshot                0
TimeBetSnapshot                    0.1
MaxSizeTimestep                    0.04
TreeDomainUpdateFrequency          0.5
DesNumNgb                          20
UnitMagneticField_in_gauss         3.54491
UnitLength_in_cm                   1
UnitMass_in_g                      1
UnitVelocity_in_cm_per_s           1
SofteningGas                       0.05
Softening_Type1                    0
Softening_Type2                    0
Softening_Type3                    0
Softening_Type4                    0
Softening_Type5                    0
