% Example compile-time options:
%  (HYDRO_MESHLESS_FINITE_MASS is optional, replace with your
%   choice of hydro/mhd options)
%
%        BOX_PERIODIC
%        BOX_SPATIAL_DIMENSION=2
%        HYDRO_MESHLESS_FINITE_MASS
%        EOS_GAMMA=(5.0/3.0)
%        MAGNETIC
%        SELFGRAVITY_OFF
%        OUTPUT_IN_DOUBLEPRECISION
%        INPUT_IN_DOUBLEPRECISION
%
InitCondFile                       mhd_blast_ics
ICFormat                           3
OutputDir                          output
TimeLimitCPU                       50000
MaxMemSize                         1600
PartAllocFactor                    5
BufferSize                         150
TimeBegin                          0
TimeMax                            1
ComovingIntegrationOn              0
BoxSize                            1
OutputListOn                       0
TimeOfFirstSnapshot                0
TimeBetSnapshot                    0.1
MaxSizeTimestep                    0.1
TreeDomainUpdateFrequency          0.5
DesNumNgb                          20
UnitLength_in_cm                   1
UnitMass_in_g                      1
UnitVelocity_in_cm_per_s           1
SofteningGas                       0.05
Softening_Type1                    0
Softening_Type2                    0
Softening_Type3                    0
Softening_Type4                    0
Softening_Type5                    0
